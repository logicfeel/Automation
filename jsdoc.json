{
    "tags": {
      "allowUnknownTags": false
    },
    "source": {
      "include": ["./src"],
      "includePattern": ".js$",
      "exclude": ["./docs"],
      "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
      "plugins/markdown"
    ],
    "opts": {
      "encoding": "utf8",
      "destination": "docs/",
      "recurse": true,
      "verbose": true
    },
    "templates": {
      "cleverLinks": false,
      "monospaceLinks": false,
      "default": {
        "outputSourceFiles": false
      }
    },
    "docdash": {
      "static": false,
      "sort": false
    }
  }