{{!-- pages/partial.hbs --}}

{{! 조각이 있는 경우 }}
{{#> part_1.task }}
  part_1 사용:기본값 (inline) => 로딩안됨
  {{#*inline "block" }}
    part_1 에서 block 오버 (inline) => 로딩안됨
  {{/inline}}  
{{/part_1.task}}

{{! 조각이 없는 경우 }}
{{#> part_1_not }}
  part_1_not 사용:기본값 (inline) .....
{{/part_1_not}}

{{! 조각이 있는 경우 : lab }}
{{#> part_lab }}
  part_lab 사용:기본값 (inline) => 로딩안됨
{{/part_lab}}

{{! 조각이 있는 경우 : lab 오버라이딩 }}
{{#> F_part_lab }}
  {{#*inline "virual" }}
    lab 에서 virual 오버 (inline) => 로딩안됨
  {{/inline}}
  {{#*inline "block" }}
    lab 에서 :: block 오버 (inline)
  {{/inline}}
{{/F_part_lab}}


{{! ************* 내부정의 *************** }}
{{#*inline "part_lab" }}
//**************************
part_lab 정의:기본값 (inline)
  {{#> part_1.task }}
  
    {{#*inline "block" }}
    block 오버 (inline)
    {{/inline}}

  {{/part_1.task }}
  {{#> part_1.task }}
  part_1 사용:기본값 (inline) => 로딩안됨
  {{/part_1.task }}
  {{#> virual }}
  virual 사용:기본값 (inline)
    {{#> part_1.task }}
  virual.part_1 사용:기본값 (inline) => 로딩안됨
    {{/part_1.task }}  
  {{/virual }}
{{/inline}}
