{{#> part_lab }}

  {{#*inline "block" }}
    lab 에서 :: block 오버 (inline)
  {{/inline}}

  {{#*inline "virual" }}
    lab 에서 virual 오버 (inline) => 로딩안됨
  {{/inline}}

{{/part_lab }}

{{! ************* 내부정의 *************** }}
{{#*inline "part_lab" }}
