{
    "tags": {
      "allowUnknownTags": false
    },
    "source": {
      "include": ["./src"],
      "includePattern": ".js$",
      "exclude": ["./docs"],
      "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
      "plugins/markdown"
    ],
    "opts": {
      "template": "node_modules/clean-jsdoc-theme",
      "theme_opts": {
        "theme": "dark"
      },
      "encoding": "utf8",
      "destination": "docs4/",
      "recurse": true,
      "verbose": true
    },
    "templates": {
      "cleverLinks": false,
      "monospaceLinks": false,
      "default": {
        "outputSourceFiles": false
      }
    },
    "docdash": {
      "static": false,
      "sort": false
    }
  }